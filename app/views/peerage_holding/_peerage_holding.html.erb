<tr>
	<%= content_tag( 'td', link_to( peerage_holding.ordinality.ordinalize, peerage_holding_show_url( :peerage_holding => peerage_holding ) ) ) %>
	<td>
		<% if peerage_holding.person.surname -%>
			<%= link_to( peerage_holding.person.surname, person_show_url( :person => peerage_holding.person ) ) %>
		<% else -%>
			-
		<% end -%>
	</td>
	<%= content_tag( 'td', link_to( peerage_holding.person.forenames, person_show_url( :person => peerage_holding.person ) ) ) %>
	<td>
		<% if peerage_holding.start_on -%>
			<%= peerage_holding.start_on.strftime( '%-d %B %Y' ) %>
		<% else -%>
			-
		<% end -%>
	</td>
	<td>
		<% if peerage_holding.end_on -%>
			<%= peerage_holding.end_on.strftime( '%-d %B %Y' ) %>
		<% else -%>
			-
		<% end -%>
	</td>
</tr>